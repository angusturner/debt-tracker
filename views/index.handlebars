<!-- ASDF -->
<div class='col-lg-6 col-md-8 col-sm-10 col-xs-12'>
  <form class='form' action='/' method='post'>
    <table class='table'>
      <thead>
        <tr>
          <td>To</td>
          <td>From</td>
          <td>Amount</td>
        </tr>
      </thead>
      <tbody>
        {{#each data}}
          <tr>
            <input type='hidden' name='id[]' value='{{id}}'>
            <td><input class='form-control' type='text' name='to[]' value='{{to}}'></td>
            <td><input class='form-control' type='text' name='from[]' value='{{from}}'></td>
            <td><input class='form-control' type='text' name='amount[]' value='{{amount}}'></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
    <input class='btn btn-primary' type='submit' name='Submit'>
  </form>
</div>

<!-- Divider -->
<div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'><br></div>

